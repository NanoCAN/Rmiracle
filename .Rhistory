rppa.serialDilution.format <-
function (spots, inducerOnlyName = T)
{
require(reshape)
title <- attr(spots, "title")
spots <- spots[!is.na(spots$DilutionFactor), ]
spots$DilutionFactor <- as.factor(spots$DilutionFactor)
spots.c <- cast(spots, CellLine + NumberOfCellsSeeded + SampleName +
SampleType + TargetGene + SpotType + SpotClass + Deposition +
Treatment + LysisBuffer + Inducer + Replicate ~ DilutionFactor,
value = "Signal", add.missing = FALSE, fun.aggregate = "median",
na.rm = T)
return(spots.c)
}
